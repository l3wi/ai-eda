{
  "name": "@ai-eda/toolkit",
  "version": "0.1.0",
  "type": "module",
  "description": "CLI for AI-assisted EDA project initialization and management",
  "bin": {
    "ai-eda": "./dist/index.js",
    "ai-eda-toolkit": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "start": "bun run ./src/index.ts",
    "dev": "bun --watch ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "kicad",
    "electronics",
    "pcb",
    "schematic",
    "eda",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-eda/common": "workspace:*",
    "@clack/prompts": "^0.9.1",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
