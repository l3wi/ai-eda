{
  "name": "claude-eda",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "CLI for AI-assisted EDA project initialization and management with KiCad",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l3wi/ai-eda.git",
    "directory": "packages/claude-eda"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "claude-eda": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "start": "bun run ./src/index.ts",
    "dev": "bun --watch ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "kicad",
    "electronics",
    "pcb",
    "schematic",
    "eda",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
